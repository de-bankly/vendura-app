{
  "name": "vendura-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js",
    "build:dev": "node scripts/build.js --env=development",
    "build:staging": "node scripts/build.js --env=staging",
    "build:prod": "node scripts/build.js --env=production",
    "analyze": "node scripts/analyze.js",
    "optimize": "node scripts/optimize.js",
    "build:optimize": "npm run build:prod && npm run optimize",
    "bundle-report": "node scripts/bundle-report.js",
    "deploy": "node scripts/deploy.js",
    "deploy:dev": "node scripts/deploy.js --env=development",
    "deploy:staging": "node scripts/deploy.js --env=staging",
    "deploy:prod": "node scripts/deploy.js --env=production",
    "deploy:build:dev": "node scripts/deploy.js --env=development --build",
    "deploy:build:staging": "node scripts/deploy.js --env=staging --build",
    "deploy:build:prod": "node scripts/deploy.js --env=production --build",
    "deploy:config": "node scripts/deploy-config.js",
    "deploy:config:dev": "node scripts/deploy-config.js --env=development",
    "deploy:config:staging": "node scripts/deploy-config.js --env=staging",
    "deploy:config:prod": "node scripts/deploy-config.js --env=production",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@grafana/faro-rollup-plugin": "^0.1.2",
    "@grafana/faro-web-sdk": "^1.14.1",
    "@grafana/faro-web-tracing": "^1.14.1",
    "@million/lint": "^1.0.14",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.5",
    "@mui/x-date-pickers": "^7.28.3",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.4.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.33.2",
    "svgo": "^3.2.0",
    "terser": "^5.29.2",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-pwa": "^0.21.1",
    "workbox-window": "^7.0.0"
  }
}
